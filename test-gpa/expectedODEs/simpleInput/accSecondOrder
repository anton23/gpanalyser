acc(As:A)^2, acc(As:B)^2, acc(As:A^2);
				
dE[acc(As:A)^2]/dt = 2* As:A acc(As:A);
dE[acc(As:B)^2]/dt = 2* As:B acc(As:B);
dE[As:A acc(As:A)]/dt = - As:A acc(As:A)* ra + As:B acc(As:A) * rb + As:A^2;
dE[As:B acc(As:B)]/dt = As:A acc(As:B)* ra - As:B acc(As:B) * rb + As:B^2;

dE[As:A acc(As:B)]/dt = - As:A acc(As:B)* ra + As:B acc(As:B) * rb + As:A As:B;
dE[As:B acc(As:A)]/dt = As:A acc(As:A)* ra - As:B acc(As:A) * rb + As:A As:B;

dE[acc(As:A^2)]/dt = As:A^2;

dE[As:A]/dt = - As:A * ra + As:B * rb;
dE[As:B]/dt = As:A * ra - As:B * rb;		
dE[As:A^2]/dt = -2*As:A^2 * ra + As:A * ra + 2*As:A As:B * rb + As:B * rb;
dE[As:B^2]/dt = -2*As:B^2 * rb + As:B * rb + 2*As:A As:B * ra + As:A * ra;
dE[As:A As:B]/dt = As:A^2 * ra - As:A As:B * ra - As:A * ra + As:B^2 * rb - As:A As:B * rb - As:B * rb;
